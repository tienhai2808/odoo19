#. module: calendar
#: model:mail.template,body_html:calendar.calendar_template_meeting_update
msgid ""
"<div style=\"font-size: 13px; color: #374151;\">\n"
"    <t t-set=\"is_online\" t-value=\"'appointment_type_id' in object and "
"object.appointment_type_id\"/>\n"
